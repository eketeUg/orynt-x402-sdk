{
  "name": "@orynt/ai-x402",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run build",
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "publish:clean": "rm -rf dist && npm run build && npm publish --access public"
  },
  "keywords": [
    "x402",
    "AI",
    "OpenAI",
    "Anthropic",
    "Grok",
    "Gradient",
    "embeddings",
    "chat",
    "TTS",
    "STT",
    "images"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eketeUg/orynt-x402-sdk"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Emmanuel Ekete",
  "license": "MIT",
  "description": "AI model wrapper using x402 payment for pay per request",
  "dependencies": {
    "cross-fetch": "^4.1.0",
    "x402-fetch": "^0.7.0"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  }
}
